{
  "files": [
    {
      "path": ".next/static/chunks/pages/_app-*.js",
      "maxSize": "250kb",
      "compression": "gzip"
    },
    {
      "path": ".next/static/chunks/pages/index-*.js",
      "maxSize": "150kb",
      "compression": "gzip"
    },
    {
      "path": ".next/static/chunks/main-*.js",
      "maxSize": "50kb",
      "compression": "gzip"
    },
    {
      "path": ".next/static/chunks/webpack-*.js",
      "maxSize": "10kb",
      "compression": "gzip"
    },
    {
      "path": ".next/static/css/*.css",
      "maxSize": "100kb",
      "compression": "gzip"
    }
  ],
  "defaultCompression": "gzip",
  "ci": {
    "trackBranches": ["main", "master"],
    "repoBranchBase": "main"
  }
}